{
  "name": "Gallery-Ideco",
  "private": true,
  "contributors": [
    {
      "name": "Savi",
      "email": "savichev95@gmail.com"
    }
  ],
  "scripts": {
    "build": "stylus --use poststylus --with '[`autoprefixer`]' --compress /app/DATA/css --out /app/DATA/css/compiled",
    "build-dev": "stylus static/css --out static/css/compiled",
    "deploy": "git push heroku master",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint **/*.styl",
    "prospector": "prospector --zero-exit",
    "prepush": "npm run lint",
    "tests": "python manage.py test",
    "watch": "stylus --watch static/css --out static/css/compiled"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "npm-run-all": "2.1.1",
    "poststylus": "0.2.3",
    "stylus": "0.54.5"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "eslint": "2.11.1",
    "husky": "0.11.4",
    "poststylus": "0.2.3",
    "stylelint": "6.5.1",
    "stylelint-config-standard": "8.0.0",
    "stylus": "0.54.5"
  },
  "engines": {
    "node": "6.0.0"
  }
}
