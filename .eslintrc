{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "jquery" : true
  },
  "extends": "eslint:recommended",
   "rules": {
     "camelcase": 2,
     "indent": [2, 4],
     "linebreak-style": [1, "unix"],
     "max-len": [2, 100],
     "max-params": [2, 3],
     "quotes": [2, "double"],
     "semi": 2
    }
}
